{
    "name": "nodejsfunction-esm-test",
    "description": "ESM specific integration tests for NodejsFunction construct",
    "private": true,
    "version": "0.0.0",
    "type": "module",
    "scripts": {
      "build": "cdk-build",
      "watch": "cdk-watch",
      "lint": "cdk-lint",
      "pkglint": "pkglint -f",
      "test": "cdk-test",
      "integ": "integ-runner --language javascript",
      "package": "cdk-package",
